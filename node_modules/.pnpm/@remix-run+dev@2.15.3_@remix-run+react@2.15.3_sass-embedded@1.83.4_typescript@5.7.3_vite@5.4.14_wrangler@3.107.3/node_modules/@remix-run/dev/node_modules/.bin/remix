#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sarvinderjitsingh/Desktop/sarvinder/bolt.diy/node_modules/.pnpm/@remix-run+dev@2.15.3_@remix-run+react@2.15.3_sass-embedded@1.83.4_typescript@5.7.3_vite@5.4.14_wrangler@3.107.3/node_modules/@remix-run/dev/dist/node_modules:/Users/sarvinderjitsingh/Desktop/sarvinder/bolt.diy/node_modules/.pnpm/@remix-run+dev@2.15.3_@remix-run+react@2.15.3_sass-embedded@1.83.4_typescript@5.7.3_vite@5.4.14_wrangler@3.107.3/node_modules/@remix-run/dev/node_modules:/Users/sarvinderjitsingh/Desktop/sarvinder/bolt.diy/node_modules/.pnpm/@remix-run+dev@2.15.3_@remix-run+react@2.15.3_sass-embedded@1.83.4_typescript@5.7.3_vite@5.4.14_wrangler@3.107.3/node_modules/@remix-run/node_modules:/Users/sarvinderjitsingh/Desktop/sarvinder/bolt.diy/node_modules/.pnpm/@remix-run+dev@2.15.3_@remix-run+react@2.15.3_sass-embedded@1.83.4_typescript@5.7.3_vite@5.4.14_wrangler@3.107.3/node_modules:/Users/sarvinderjitsingh/Desktop/sarvinder/bolt.diy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sarvinderjitsingh/Desktop/sarvinder/bolt.diy/node_modules/.pnpm/@remix-run+dev@2.15.3_@remix-run+react@2.15.3_sass-embedded@1.83.4_typescript@5.7.3_vite@5.4.14_wrangler@3.107.3/node_modules/@remix-run/dev/dist/node_modules:/Users/sarvinderjitsingh/Desktop/sarvinder/bolt.diy/node_modules/.pnpm/@remix-run+dev@2.15.3_@remix-run+react@2.15.3_sass-embedded@1.83.4_typescript@5.7.3_vite@5.4.14_wrangler@3.107.3/node_modules/@remix-run/dev/node_modules:/Users/sarvinderjitsingh/Desktop/sarvinder/bolt.diy/node_modules/.pnpm/@remix-run+dev@2.15.3_@remix-run+react@2.15.3_sass-embedded@1.83.4_typescript@5.7.3_vite@5.4.14_wrangler@3.107.3/node_modules/@remix-run/node_modules:/Users/sarvinderjitsingh/Desktop/sarvinder/bolt.diy/node_modules/.pnpm/@remix-run+dev@2.15.3_@remix-run+react@2.15.3_sass-embedded@1.83.4_typescript@5.7.3_vite@5.4.14_wrangler@3.107.3/node_modules:/Users/sarvinderjitsingh/Desktop/sarvinder/bolt.diy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
